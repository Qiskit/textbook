[MESSAGES CONTROL]

disable=
    wrong-import-order,
    wrong-import-position,
    ungrouped-imports,
    reimported,
    import-outside-toplevel,
    pointless-statement,
    expression-not-assigned,
    missing-module-docstring,
    redefined-outer-name

good-names=
    qc,
    qr,
    cr

additional-builtins=
    _,
    Out,
    display

[FORMAT]
max-line-length=75
module-naming-style=any

[TYPECHECK]
ignored-classes=
    QuantumCircuit
